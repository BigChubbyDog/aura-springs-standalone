{
  "name": "aura-spring-cleaning",
  "version": "1.0.0",
  "private": true,
  "description": "Austin's Premier Luxury Cleaning Service - Downtown High-Rise Specialists",
  "author": "Aura Spring Cleaning",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true next build",
    "sitemap": "next-sitemap",
    "postbuild": "next-sitemap",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-to-azure.ps1",
    "deploy:staging": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-to-azure.ps1 -Environment staging",
    "deploy:prod": "npm run build && npm run deploy",
    "azure:login": "az login --service-principal -u $AZURE_CLIENT_ID -p $AZURE_CLIENT_SECRET --tenant $AZURE_TENANT_ID",
    "azure:deploy": "az webapp deployment source config-zip --resource-group rg-auraspringcleaning-prod --name auraspringcleaning --src deploy.zip"
  },
  "dependencies": {
    "@azure/identity": "^4.11.1",
    "@hookform/resolvers": "^3.6.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "axios": "^1.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.52.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.10.0",
    "square": "^43.0.1",
    "stripe": "^18.4.0",
    "swr": "^2.2.0",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "next-sitemap": "^4.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
